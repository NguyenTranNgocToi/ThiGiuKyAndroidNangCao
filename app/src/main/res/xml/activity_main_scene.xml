<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe motion:dragDirection="dragDown" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="380dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="48dp"
            android:layout_marginStart="48dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView2"
            motion:layout_constraintStart_toEndOf="@+id/edA"
            motion:layout_constraintBottom_toBottomOf="@+id/edA"
            android:layout_marginLeft="40dp"
            android:layout_marginStart="40dp"
            motion:layout_constraintTop_toTopOf="@+id/edA" />
        <Constraint
            android:id="@+id/edA"
            motion:layout_constraintStart_toEndOf="@+id/textView"
            android:layout_width="100dp"
            android:layout_height="70dp"
            motion:layout_constraintBottom_toBottomOf="@+id/textView"
            motion:layout_constraintVertical_bias="0.279"
            android:layout_marginStart="40dp"
            motion:layout_constraintTop_toTopOf="@+id/textView" />
        <Constraint
            android:id="@+id/edA"
            motion:layout_constraintStart_toEndOf="@+id/textView"
            android:layout_width="50dp"
            android:layout_height="70dp"
            motion:layout_constraintBottom_toBottomOf="@+id/textView"
            motion:layout_constraintVertical_bias="0.279"
            android:layout_marginStart="40dp"
            motion:layout_constraintTop_toTopOf="@+id/textView" />
        <Constraint
            android:id="@+id/edB"
            motion:layout_constraintStart_toEndOf="@+id/textView2"
            android:layout_width="100dp"
            android:layout_height="70dp"
            motion:layout_constraintBottom_toBottomOf="@+id/textView2"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginStart="40dp"
            motion:layout_constraintTop_toTopOf="@+id/textView2" />
        <Constraint
            android:id="@+id/edB"
            motion:layout_constraintStart_toEndOf="@+id/textView2"
            android:layout_width="50dp"
            android:layout_height="70dp"
            motion:layout_constraintBottom_toBottomOf="@+id/textView2"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginStart="40dp"
            motion:layout_constraintTop_toTopOf="@+id/textView2" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            motion:layout_constraintStart_toStartOf="@+id/textView"
            motion:layout_constraintEnd_toEndOf="@+id/textView"
            android:layout_marginTop="24dp"
            motion:layout_constraintHorizontal_bias="0.515" />
        <Constraint
            android:layout_height="84dp"
            motion:layout_constraintTop_toTopOf="@+id/buttonCong"
            motion:layout_constraintBottom_toBottomOf="@+id/buttonCong"
            android:layout_width="82dp"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintStart_toEndOf="@+id/buttonCong"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp"
            android:id="@+id/buttonNhan" />
        <Constraint
            android:layout_height="84dp"
            android:layout_width="82dp"
            android:id="@+id/buttonTru"
            motion:layout_constraintStart_toEndOf="@+id/buttonNhan"
            motion:layout_constraintBottom_toBottomOf="@+id/buttonNhan"
            motion:layout_constraintTop_toTopOf="@+id/buttonNhan"
            android:layout_marginStart="20dp" />
        <Constraint
            android:layout_height="84dp"
            motion:layout_constraintTop_toTopOf="@+id/buttonTru"
            motion:layout_constraintBottom_toBottomOf="@+id/buttonTru"
            android:layout_width="82dp"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintStart_toEndOf="@+id/buttonTru"
            android:layout_marginLeft="28dp"
            android:layout_marginStart="20dp"
            android:id="@+id/button4" />
        <Constraint
            android:layout_height="84dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="60dp"
            android:layout_width="82dp"
            android:layout_marginStart="12dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView3"
            android:id="@+id/buttonCong"
            android:layout_marginLeft="12dp" />
    </ConstraintSet>
</MotionScene>